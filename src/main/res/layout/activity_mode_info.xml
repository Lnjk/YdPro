            </LinearLayout>

            <LinearLayout style="@style/price_ll2">

                <TextView
                    style="@style/price_name"
                    android:text="设为默认:" />
                <TextView
                    android:id="@+id/tv_move_ismr"
                    style="@style/design_editext"
                    />
                <ImageButton
                    android:id="@+id/ib_move_sfmr"
                    style="@style/price_button_down" />

            </LinearLayout>
        </LinearLayout>

        <View style="@style/price_view" />





    </LinearLayout>
    <!--</ScrollView>-->



    <LinearLayout
        style="@style/price_ll1"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btn_move_save"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/button"
            android:text="保存" />


    </LinearLayout>

</LinearLayout>